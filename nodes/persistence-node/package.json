    "@polywrap/package-validation": "0.0.1-prealpha.88",
    "awilix": "6.0.0",
    "axios": "0.24.0",
    "commander": "8.3.0",
    "connect-timeout": "1.9.0",
    "cors": "2.8.5",
    "ethers": "5.5.2",
    "express": "4.17.2",
    "ipfs-core": "0.14.1",
    "ipfs-http-client": "56.0.1",
    "multer": "1.4.4",
    "mustache": "4.2.0",
    "mustache-express": "1.3.2",
    "typescript": "4.4.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.11",
    "copyfiles": "2.4.1",
    "jest": "27.1.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.3.0",
    "@polywrap/test-env-js": "0.0.1-prealpha.88"
  }
}
