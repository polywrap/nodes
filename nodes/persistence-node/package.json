    "cors": "^2.8.5",
    "ethers": "^5.5.2",
    "express": "^4.17.2",
    "ipfs-core": "0.14.1",
    "ipfs-http-client": "56.0.1",
    "multer": "^1.4.4",
    "mustache": "^4.2.0",
    "mustache-express": "^1.3.2",
    "typescript": "^4.4.2"
  },
